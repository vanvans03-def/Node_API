{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "headers": {
          "x-vercel-cron": "1"
        }
      }
    ],
   
    "crons": [{
      "path": "/api/cron",
      "schedule": "45 16 * * *"
    }]
  }
  